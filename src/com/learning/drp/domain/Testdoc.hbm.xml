<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.learning.drp.domain.Testdoc" table="testdoc">
		<id name="id">
			<generator class="native"/>
		</id>
		<property name="testDocName"/>
		<property name="testDocPath"/>
		<property name="testDocAnswer"/>
		<property name="testDocDescription"/>
		<property name="userId"/>
		<property name="createTime"/>
	</class>
</hibernate-mapping>
